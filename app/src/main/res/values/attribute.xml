<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="switchView">
        <attr name="buttonColor"  format="color"/>
        <attr name="borderColor"  format="color"/>
        <attr name="leftColor"  format="color"/>
        <attr name="rightColor"  format="color"/>
        <attr name="leftImage"  format="reference"/>
        <attr name="rightImage"  format="reference"/>
        <attr name="buttonImage"  format="reference"/>
        <attr name="ratio" format="enum">
            <enum name="two" value="2" />
            <enum name="four" value="4"/>
            <enum name="six" value="6"/>
            <enum name="seven" value="7"/>
        </attr>
        <attr name="extraRadius" format="dimension"/>
        <attr name="borderThickness" format="dimension"/>
        <attr name="shadowColor" format="color"/>
        <attr name="isOn" format="boolean"/>

    </declare-styleable>
